    "jest": {
        "collectCoverage": true,
        "testURL": "http://localhost/",
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/app.tsx",
            "!src/**/*.stories.{js,jsx,ts,tsx}",
            "!src/**/*.mock.{js,jsx,ts,tsx}",
            "!src/**/*.constant.{js,jsx,ts,tsx}",
            "!dist/**/*.js",
            "!dist/*.js",
            "!src/*.scss",
            "!src/*.less",
            "!src/*.svg"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|less|scss)$": "babel-jest",
            "@/shared/(.*)$": "<rootDir>/src/shared/$1",
            "@/store/(.*)$": "<rootDir>/src/store/$1",
            "@/reducers/(.*)$": "<rootDir>/src/store/reducers/$1",
            "@/action/(.*)$": "<rootDir>/src/store/action/$1",
            "@/atoms/(.*)$": "<rootDir>/src/shared/components/atoms/$1",
            "@/molecules/(.*)$": "<rootDir>/src/shared/components/molecules/$1",
            "@/organism/(.*)$": "<rootDir>/src/shared/components/organism/$1",
            "@/templates/(.*)$": "<rootDir>/src/shared/components/templates/$1",
            "@/interfaces/(.*)$": "<rootDir>/src/shared/interfaces/$1",
            "@/utils/(.*)$": "<rootDir>/src/shared/utils/$1",
            "@/abstract/(.*)$": "<rootDir>/src/shared/abstract/$1",
            "@/constant/(.*)$": "<rootDir>/src/shared/constant/$1",
            "@/scss/(.*)$": "<rootDir>/src/shared/styles/scss/$1",
            "@/styled-component/(.*)$": "<rootDir>/src/shared/styles/styled-component/$1",
            "@/router/(.*)$": "<rootDir>/src/router/$1",
            "^config/(.*)$": "<rootDir>/etc/jest/$1"
        },
        "coverageDirectory": ".jest_coverage",
        "moduleFileExtensions": [
            "js",
            "jsx",
            "es6",
            "ts",
            "tsx"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest",
            "^.+\\.jsx$": "babel-jest",
            "^.+\\.ts$": "ts-jest",
            "^.+\\.tsx$": "ts-jest",
            "^.+\\.es6$": "babel-jest"
        },
        "transformIgnorePatterns": [
            "node_modules/"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/etc/jest/enzyme.config.ts"
        ]
    }
